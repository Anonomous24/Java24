class fact {
  public static void main(String[] args) {
    int n = args.length, i, num;
    long fact;
    for (i=0;i<n;i++) {
      num = Integer.parseInt(args[i]);
      fact = 1;
      while(num>1) {
        fact *= num;
        num--;
      }
      System.out.println("factorial of " + args[i] + " is " + fact);
    }
  }
}
